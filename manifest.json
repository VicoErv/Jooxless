{
    "manifest_version": 2,
    "name": "Jooxless",
    "version": "1.0",
    "description": "Joox web helper.",
    "homepage_url": "https://www.joox.com",
    "permissions": ["activeTab", "*://*.joox.com/*"],
    "browser_action": {
        "default_popup": "src/popup.html"
    },
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}